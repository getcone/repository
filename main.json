{
	"name": "Cone Main Repository",
	"cone_min": "0.10.0",
	"packages": [
		{
			"name": "composer",
			"version": "1.8.5",
			"prerequisites": [
				{
					"type": "command_not_found",
					"value": "composer"
				}
			],
			"install": [
				{
					"type": "download",
					"target": "composer.phar",
					"url": "https://getcomposer.org/download/1.8.5/composer.phar",
					"hash": {
						"sha256": "4e4c1cd74b54a26618699f3190e6f5fc63bb308b13fa660f71f2a2df047c0e17"
					}
				},
				{
					"type": "keep",
					"file": "composer.phar",
					"as": "composer.phar"
				}
			],
			"shortcuts": {
				"composer": {
					"target": "php",
					"target_arguments": [
						{
							"path": "composer.phar"
						}
					]
				}
			}
		},
		{
			"name": "ffmpeg",
			"display_name": "ffmpeg",
			"aliases": [
				"ffprobe",
				"ffplay"
			],
			"version": "4.2",
			"prerequisites": [
				{
					"type": "command_not_found",
					"value": "ffmpeg"
				}
			],
			"install": [
				{
					"type": "platform_download_and_extract",
					"target": "tmp",
					"windows_x64": {
						"url": "https://ffmpeg.zeranoe.com/builds/win64/static/ffmpeg-{version}-win64-static.zip"
					},
					"windows_x86": {
						"url": "https://ffmpeg.zeranoe.com/builds/win32/static/ffmpeg-{version}-win32-static.zip"
					},
					"macos": {
						"url": "https://ffmpeg.zeranoe.com/builds/macos64/static/ffmpeg-{version}-macos64-static.zip"
					},
					"linux_x64": {
						"url": "https://johnvansickle.com/ffmpeg/releases/ffmpeg-release-amd64-static.tar.xz"
					},
					"linux_x86": {
						"url": "https://johnvansickle.com/ffmpeg/releases/ffmpeg-release-i686-static.tar.xz"
					}
				},
				{
					"type": "platform_switch",
					"windows_x64": [
						{
							"type": "keep",
							"file": "tmp/ffmpeg-{version}-win64-static/bin",
							"as": ""
						}
					],
					"windows_x86": [
						{
							"type": "keep",
							"file": "tmp/ffmpeg-{version}-win32-static/bin",
							"as": ""
						}
					],
					"macos": [
						{
							"type": "keep",
							"file": "tmp/ffmpeg-{version}-macos64-static/bin",
							"as": ""
						}
					],
					"linux_x64": [
						{
							"type": "keep",
							"file": "tmp/ffmpeg-release-amd64-static/ffmpeg",
							"as": "ffmpeg"
						},
						{
							"type": "keep",
							"file": "tmp/ffmpeg-release-amd64-static/ffprobe",
							"as": "ffprobe"
						}
					],
					"linux_x86": [
						{
							"type": "keep",
							"file": "tmp/ffmpeg-release-i686-static/ffmpeg",
							"as": "ffmpeg"
						},
						{
							"type": "keep",
							"file": "tmp/ffmpeg-release-i686-static/ffprobe",
							"as": "ffprobe"
						}
					]
				},
				{
					"type": "delete",
					"file": "tmp"
				}
			],
			"shortcuts": {
				"ffmpeg": {
					"target": "ffmpeg",
					"target_winext": ".exe"
				},
				"ffprobe": {
					"target": "ffprobe",
					"target_winext": ".exe"
				},
				"ffplay": {
					"target": "ffplay",
					"target_winext": ".exe"
				}
			}
		},
		{
			"name": "maven",
			"aliases": [
				"mvn"
			],
			"version": "3.6.1",
			"prerequisites": [
				{
					"type": "command_not_found",
					"value": "mvn"
				}
			],
			"dependencies": [
				"openjdk"
			],
			"install": [
				{
					"type": "platform_download_and_extract",
					"target": "tmp",
					"windows": {
						"url": "http://mirror.easyname.ch/apache/maven/maven-3/{version}/binaries/apache-maven-{version}-bin.zip"
					},
					"unix": {
						"url": "http://mirror.easyname.ch/apache/maven/maven-3/{version}/binaries/apache-maven-{version}-bin.tar.gz"
					}
				},
				{
					"type": "keep",
					"file": "tmp/apache-maven-{version}",
					"as": ""
				},
				{
					"type": "delete",
					"file": "tmp"
				}
			],
			"shortcuts": {
				"mvn": {
					"target": "bin/mvn",
					"target_winext": ".cmd"
				},
				"mvnDebug": {
					"target": "bin/mvnDebug",
					"target_winext": ".cmd"
				}
			}
		},
		{
			"name": "nose",
			"version": "1.1.0",
			"install": [
				{
					"type": "platform_download_and_extract",
