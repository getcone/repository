{
	"name": "Cone Main Repository",
	"cone_min": "0.10.0",
	"packages": [
		{
			"name": "cipherfinder",
			"version": "0.1.0",
			"dependencies": [
				"composer"
			],
			"install": [
				{
					"type": "platform_download_and_extract",
					"target": "tmp",
					"windows": {
						"url": "https://github.com/hell-sh/cipherfinder/archive/v{version}.zip"
					},
					"unix": {
						"url": "https://github.com/hell-sh/cipherfinder/archive/v{version}.tar.gz"
					}
				},
				{
					"type": "keep",
					"file": "tmp/cipherfinder-{version}",
					"as": ""
				},
				{
					"type": "delete",
					"file": "tmp"
				}
			],
			"shortcuts": {
				"cipherfinder": {
					"target": "php",
					"target_arguments": [
						{
							"path": "cipherfinder.php"
						}
					]
				}
			}
		},
		{
			"name": "composer",
			"version": "1.9.1",
			"prerequisites": [
				{
					"type": "command_not_found",
					"value": "composer"
				}
			],
			"install": [
				{
					"type": "download",
					"target": "composer.phar",
					"url": "https://getcomposer.org/download/{version}/composer.phar",
					"hash": {
						"sha256": "1f210b9037fcf82670d75892dfc44400f13fe9ada7af9e787f93e50e3b764111"
					}
				},
				{
					"type": "keep",
					"file": "composer.phar",
					"as": "composer.phar"
				}
			],
			"shortcuts": {
				"composer": {
					"target": "php",
					"target_arguments": [
						{
							"path": "composer.phar"
						}
					]
				}
			}
		},
		{
			"name": "doxygen",
			"aliases": [
				"doxyindexer",
				"doxywizard"
			],
			"version": "1.8.16",
			"prerequisites": [
				{
					"type": "command_not_found",
					"value": "doxygen"
				}
			],
			"install": [
				{
					"type": "platform_switch",
					"windows": [
						{
							"type": "platform_download_and_extract",
							"target": "tmp",
							"windows_x64": {
								"url": "http://doxygen.nl/files/doxygen-{version}.windows.x64.bin.zip"
							},
							"windows_x86": {
								"url": "http://doxygen.nl/files/doxygen-{version}.windows.bin.zip"
							}
						},
						{
							"type": "keep",
							"file": "tmp",
							"as": ""
						}
					],
					"macos": [
						{
							"type": "download",
							"target": "Doxygen.dmg",
							"url": "http://doxygen.nl/files/Doxygen-1.8.16.dmg"
						},
						{
							"type": "keep",
							"file": "Doxygen.dmg",
							"as": "Doxygen.dmg"
						}
					],
					"linux": [
						{
							"type": "download",
							"target": "doxygen.tar.gz",
							"url": "http://doxygen.nl/files/doxygen-1.8.16.linux.bin.tar.gz"
						},
						{
							"type": "extract",
							"file": "doxygen.tar.gz",
							"target": "tmp"
						},
						{
							"type": "delete",
							"file": "doxygen.tar.gz"
						},
						{
							"type": "keep",
							"file": "tmp/doxygen-{version}/bin",
							"as": ""
						},
						{
							"type": "delete",
							"file": "tmp"
						}
					]
				}
			],
			"shortcuts": {
				"doxygen": {
					"target": "doxygen",
					"target_winext": ".exe"
				},
				"doxyindexer": {
					"target": "doxyindexer",
					"target_winext": ".exe"
				},
				"doxywizard": {
					"target": "doxywizard",
					"target_winext": ".exe"
				}
			}
		},
		{
			"name": "ffmpeg",
