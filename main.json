					"as": "nose"
				},
				{
					"type": "delete",
					"file": "tmp"
				}
			],
			"shortcuts": {
				"nose": {
					"target": "php",
					"target_arguments": [
						{
							"path": "nose"
						}
					]
				}
			}
		},
